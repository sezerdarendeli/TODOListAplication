@using TODOListApplication.Web.Models
@model  ToDoListViewModel
@{
    ViewBag.Title = "_GetToDoList";
}



<ul class="nav">

    @if (Model.ToDoList != null)
    {
        foreach (var item in Model.ToDoList)
        {

            <li class="nav-item">
                <a class="nav-link " href="@Url.Action("GetToDo", "ToDo",new {id= item.Id})">
                    <i class="material-icons ">dashboard</i>
                    <p>@item.Name</p>
                </a>
            </li>
        }
        <li class="nav-item">
            <a class="nav-link" href="@Url.Action("Add", "ToDo")">
                <i class="material-icons ">add</i>
                <p>Add To-Do List </p>
            </a>
        </li>

    }

</ul>